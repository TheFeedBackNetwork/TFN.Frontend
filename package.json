{
  "name": "the-feed-back-network-frontend",
  "description": "The FeedbackNetworks frontend",
  "version": "1.0.0",
  "author": "Lutando Ngqakaza <lutz@thefeedbacknetwork.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "es6",
    "hmr",
    "hot",
    "babel",
    "webpack",
    "sentry",
    "architecture",
    "webpack-dev-middleware",
